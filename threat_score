threat_score = 0

target_distance = 500
if target_distance <= 600:
    threat_score = threat_score + 50

    target_speed = 400
    if target_speed >=350:
        threat_score = threat_score + 70

        target_type = ("missile")
        if target_type == "missile":
            threat_score = threat_score + 30
             
print("Threat Score:" , threat_score)
